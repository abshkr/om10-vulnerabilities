<?xml version="1.0" encoding="utf-8"?>
<s:GridItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark" 
					xmlns:mx="library://ns.adobe.com/flex/mx" clipAndEnableScrolling="true">
	
	<fx:Script>
		<![CDATA[
			override public function prepare(hasBeenRecycled:Boolean):void 
			{
				var cmpt_num:int=0;
				if (data != null)
				{
					if (data[column.dataField])
					{
						
						if ((data[column.dataField]!=null) && (data[column.dataField]!='null') && (data[column.dataField]!=''))
						{
							cmpt_num = (data[column.dataField]).length;
						}
					}
				}
				cmpt_details.text = String(cmpt_num) + " Compartments";
			}
		]]>
	</fx:Script>
	
	<s:Group width="100%" height="100%" mouseEnabledWhereTransparent="true">
		<s:Label id="cmpt_details" verticalCenter="0" horizontalCenter="0"/>
	</s:Group>
	
</s:GridItemRenderer>

