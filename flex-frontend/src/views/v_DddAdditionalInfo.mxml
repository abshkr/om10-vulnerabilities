				var fields:Object;
				var types:Object;
				
				fields = new Object();
				types = new Object();
				
				fields["ddd_dd_number"] = ddd_dd_number;
				types["ddd_dd_number"] = "EXACT_STRING";
				fields["ddd_dd_supp_code"] = ddd_dd_supp_code;
				types["ddd_dd_supp_code"] = "EXACT_STRING";
				fields["ddd_dd_tripord_no"] = ddd_dd_tripord_no;
				types["ddd_dd_tripord_no"] = "NUMBER";
				fields["ddd_dd_ld_type"] = ddd_dd_ld_type;
				types["ddd_dd_ld_type"] = "NUMBER";
				fields["ddd_templ_id"] = ddd_templ_id;
				types["ddd_templ_id"] = "EXACT_STRING";
				//fields["addi_fld_line_no"] = addi_fld_line_no;
				//types["addi_fld_line_no"] = "NUMBER";
				
				//currentPage=1;
				pagedRecords.currentPage=1;
				pagedRecords.removeAll();
				pagedRecords.refresh();
				//this.pagedRecords.service( fields, types, currentSorts, currentOrders, currentPage, currentPageSize );
				this.pagedRecords.listService( fields, types, currentSorts, currentOrders );
			}
			
