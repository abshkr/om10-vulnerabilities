				po["ddd_templ_id"] = tools.getObjAttribute( params, "ddd_templ_id", "" );
				
				
				var fields:Object;
				var types:Object;
				
				//interruptor=false;
				pagedRecords.interruptor=false;
				
				fields = new Object();
				types = new Object();
				
				fields["ddd_dd_number"] = po["ddd_dd_number"];
				types["ddd_dd_number"] = "NUMBER";
				fields["ddd_dd_supp_code"] = po["ddd_dd_supp_code"];
				types["ddd_dd_supp_code"] = "EXACT_STRING";
				fields["ddd_dd_tripord_no"] = po["ddd_dd_tripord_no"];
				types["ddd_dd_tripord_no"] = "NUMBER";
				fields["ddd_dd_ld_type"] = po["ddd_dd_ld_type"];
				types["ddd_dd_ld_type"] = "NUMBER";
				fields["ddd_templ_id"] = po["ddd_templ_id"];
				types["ddd_templ_id"] = "EXACT_STRING";
				
				currentFields = fields;
				currentTypes = types;
				this.startLoadingPages();
				
				if ( po.hasOwnProperty( "ddd_dd_supp_code" ) )
				{
					this.fltr_supplier.selectedIndex = tools.getItemIndexFromCode( po["ddd_dd_supp_code"], supplierList, "cmpy_code");
