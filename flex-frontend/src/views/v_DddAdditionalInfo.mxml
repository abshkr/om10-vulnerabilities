			private function startFiltering():void
			{
				var fields:Object;
				var types:Object;
				
				//interruptor=false;
				pagedRecords.interruptor=false;
				
				fields = new Object();
				types = new Object();
				
				if (fltr_supplier.selectedIndex > -1) 
				{
					fields["ddd_dd_supp_code"] = supplierList[fltr_supplier.selectedIndex].cmpy_code;
					if ( fields["ddd_dd_supp_code"] == "ANY" )
					{
						fields["ddd_dd_supp_code"] = "";
					}
				}
				else
				{
					if ( global.setComboBoxNoSelection == true )
					{
						fltr_supplier.selectedIndex = -1;
					}
					fields["ddd_dd_supp_code"] = "";
				}
				types["ddd_dd_supp_code"] = "EXACT_STRING";
				
				if (fltr_loadtype.selectedIndex > -1) 
				{
					fields["ddd_dd_ld_type"] = loadTypeList[fltr_loadtype.selectedIndex].load_type_id;
				}
				else
				{
					if ( global.setComboBoxNoSelection == true )
					{
						fltr_loadtype.selectedIndex = -1;
					}
					fields["ddd_dd_ld_type"] = "";
				}
				types["ddd_dd_ld_type"] = "EXACT_STRING";
				
				
				if(fltr_ddnumber.text!='')
				{					
					fields["ddd_dd_number"] = fltr_ddnumber.text;
				}
				else
				{
					fields["ddd_dd_number"] = "";
				}
				types["ddd_dd_number"] = "STRING";
				
				if(fltr_triporder.text!='')
				{					
					fields["ddd_dd_tripord_no"] = fltr_triporder.text;
				}
				else
				{
					fields["ddd_dd_tripord_no"] = "";
				}
				types["ddd_dd_tripord_no"] = "STRING";
				
				if(fltr_dddtemp.text!='')
				{					
					fields["ddd_templ_id"] = fltr_dddtemp.text;
				}
				else
				{
					fields["ddd_templ_id"] = "";
				}
				types["ddd_templ_id"] = "STRING";
				
				currentFields = fields;
				currentTypes = types;
				
				this.startLoadingPages();
				
				trace("this.grid.scroller3333", this.dataGrid.scroller);
				trace("this.grid.scroller.verticalScrollBar.value", this.dataGrid.scroller.verticalScrollBar.value);
			}
			
