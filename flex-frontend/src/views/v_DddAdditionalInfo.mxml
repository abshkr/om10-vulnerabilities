				}
				
				this.parentParams = params;
				
				currentState = "normal";
				
				trace ("...............................in setFilters");
				tools.pr( params);
				
				var po:Object=new Object();
				
				po["ddd_dd_number"] = tools.getObjAttribute( params, "ddd_dd_number", "" );
				po["ddd_dd_supp_code"] = tools.getObjAttribute( params, "ddd_dd_supp_code", "" );
