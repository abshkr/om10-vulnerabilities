<?xml version="1.0" encoding="utf-8"?>
<s:GridItemRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
					xmlns:s="library://ns.adobe.com/flex/spark" 
					xmlns:mx="library://ns.adobe.com/flex/mx" clipAndEnableScrolling="true">
	
	<fx:Script>
		<![CDATA[
			override public function prepare(hasBeenRecycled:Boolean):void 
			{
				if (data != null)
				{
					if (data[column.dataField])
					{
						
						if ((data[column.dataField]!=null) && (data[column.dataField]!='null') && (data[column.dataField]!=''))
						{
							list.dataProvider=data[column.dataField];
						}
					}
				}
			}
		]]>
	</fx:Script>
	
	<s:List id="list" itemRenderer="renderers.CompositionListRenderer" contentBackgroundAlpha="0" borderVisible="false">
		<s:layout>
			<s:HorizontalLayout gap="0"/>
		</s:layout>
	</s:List>
</s:GridItemRenderer>
