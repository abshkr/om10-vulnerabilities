				}
				*/
			}
			
			protected function closeoutRangeLabelFunction(item:Object, column:GridColumn):String
			{
				var ret:String = "";
				for each(var obj:Object in this.closeoutRangeList)
				{
					if(obj.id == item [column.dataField]) ret=obj.text;
				}
				return ret;
			}
			
