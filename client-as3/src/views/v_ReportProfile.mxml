			private function createRecord():void
			{
				var prod_num:int;
				
				prod_num = reportProfileKeyChecker.dataInteger;
				if ( prod_num > 0 )
				{
					var errorBox:MessageDialog = new MessageDialog( resourceManager.getString('default','ALERT__REPORT_PROFILE_EXIST') );
					this.btn_save.enabled = this.canCreate;
					return;
				}
				
				if ( this.checkFields() > 0 )
				{
					this.btn_save.enabled = this.canCreate;
					return;
				}
				
				var obj:Object = new Object();
				
				
				obj.report_file = report_file.text;
				obj.report_name = report_name.text;
				
				if (report_type.selectedIndex>=0)
				{
					obj.report_type = reportProfileTypeList[report_type.selectedIndex].report_type_code;
					obj.report_type_name = reportProfileTypeList[report_type.selectedIndex].report_type_name;
				}
				else
				{
					obj.report_type = 'N';
					obj.report_type_name = '';
				}
				
				if (report_closeoutby.selectedIndex>=0)
				{
					obj.report_closeoutby = closeoutRangeList[report_closeoutby.selectedIndex].id;
					obj.report_closeoutby_name =closeoutRangeList[report_closeoutby.selectedIndex].text;
				}
				else
				{
					obj.report_closeoutby = "";
					obj.report_closeoutby_name = "";
				}
				
				obj.report_jasper_file = report_jasper_file.text;
				/*
				if (report_jasper_file.selectedIndex>=0)
				{
					obj.report_jasper_file = this.reportProfileFileListFromFolder[report_jasper_file.selectedIndex].report_jasper_file;
				}
				else
				{
					obj.report_jasper_file = "";
				}
				*/
				prod_num = reportJasperKeyChecker.dataInteger;
				if ( prod_num > 0 )
				{
					var errorBox:MessageDialog = new MessageDialog( '[' + obj.report_jasper_file + ', ' + obj.report_type_name + '] ' + resourceManager.getString('default','ALERT__REPORT_JASPER_TYPE_USED') );
					this.btn_save.enabled = this.canCreate;
					return;
				}
				
				obj.report_desc = report_desc.text;
				obj.report_ondemand_title = report_ondemand_title.text;
				
				obj.report_closeout_flag = (report_closeout_flag.selected==true?"yes":"");
				obj.report_ondemand_flag = (report_ondemand_flag.selected==true?"yes":"");

				createAction.service( obj );
			}
			
			protected function createData_resultHandler( ):void
			{
				clearDetail();
				if ( this.createAction.dataString == "OK" )
				{
					global.msgSuccess(resourceManager.getString('default','MSG_CREATE__SUCCESS'));
					this.reportProfileFileList.service( );
					this.reportProfileFileListFromFolder.service( );
					this.reportProfileFileListFromDatabase.service( );
				}
				else
				{
					global.msgFail(resourceManager.getString('default','ERROR_CREATE__FAILURE'));
