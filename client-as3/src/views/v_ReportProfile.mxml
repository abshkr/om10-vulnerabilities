			protected function prepareJasperFileList( curr_file:String="" ):void
			{
				this.jasperFileList.removeAll();
				
				var obj:Object = new Object();
				if ( curr_file.length > 0 )
				{
					obj['report_jasper_file'] = curr_file;
					this.jasperFileList.addItem( obj );
				}
				
				for each ( var o:Object in this.reportProfileFileList.source )
				{
					obj = new Object();
					obj['report_jasper_file'] = o['report_jasper_file'];
					this.jasperFileList.addItem( obj );
				}
				
			}
			
