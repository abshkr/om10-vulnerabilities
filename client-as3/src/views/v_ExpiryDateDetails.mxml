			private function startFiltering():void
			{
				var fields:Object;
				var types:Object;
				
				//interruptor=false;
				pagedRecords.interruptor=false;
				
				fields = new Object();
				types = new Object();
				
				
				if(fltr_object.text!='')
				{					
					fields["ed_object_id"] = fltr_object.text;
				}
				else
				{
					fields["ed_object_id"] = this.currentExpiryObjectId;
				}
				types["ed_object_id"] = "STRING";
				
				if ( this.currentExpiryTargetCode!='' )
				{					
					fields["ed_target_code"] = this.currentExpiryTargetCode;
					types["ed_target_code"] = "EXACT_STRING";
				}
				else
				{
					if (fltr_target.selectedIndex > -1) 
					{
						fields["ed_target_code"] = this.expiryDateTargetList[fltr_target.selectedIndex].edta_target_code;
					}
					else
					{
						if ( global.setComboBoxNoSelection == true )
						{
							fltr_target.selectedIndex = -1;
						}
						fields["ed_target_code"] = "";
					}
					types["ed_target_code"] = "EXACT_STRING";
				}
				
				if ( this.currentExpiryCompanyCode!='' )
				{					
					fields["ed_cmpy_code"] = this.currentExpiryCompanyCode;
					types["ed_cmpy_code"] = "EXACT_STRING";
				}
				else
				{
					if (fltr_company.selectedIndex > -1) 
					{
						fields["ed_cmpy_code"] = this.targetCompanyList[fltr_company.selectedIndex].cmpy_code;
					}
					else
					{
						if ( global.setComboBoxNoSelection == true )
						{
							fltr_company.selectedIndex = -1;
						}
						fields["ed_cmpy_code"] = "";
					}
					types["ed_cmpy_code"] = "EXACT_STRING";
				}
				
				
				currentFields = fields;
				currentTypes = types;
				
				this.startLoadingPages();
				
				trace("this.grid.scroller3333", this.dataGrid.scroller);
				trace("this.grid.scroller.verticalScrollBar.value", this.dataGrid.scroller.verticalScrollBar.value);
