			private function startFiltering():void
			{
				var fields:Object;
				var types:Object;
				
				//interruptor=false;
				pagedRecords.interruptor=false;
				
				fields = new Object();
				types = new Object();
				
				
				if(fltr_meter.text!='')
				{					
					fields["bam_code||bam_name"] = fltr_meter.text+"||"+fltr_meter.text;
				}
				else
				{
					fields["bam_code||bam_name"] = "";
				}
				types["bam_code||bam_name"] = "STRING||STRING";
				
				
				if ( this.currentState != "widget" )
				{
					if (fltr_usage.selectedIndex > -1) 
					{
						fields["bam_usage_id"] = loadMeterUsageList[fltr_usage.selectedIndex].bam_usage_id;
					}
					else
					{
						if ( global.setComboBoxNoSelection == true )
						{
							fltr_usage.selectedIndex = -1;
						}
						fields["bam_usage_id"] = "";
					}
					types["bam_usage_id"] = "EXACT_STRING";
					
					if (fltr_metertype.selectedIndex > -1) 
					{
						fields["bam_type_id"] = meterTypeList[fltr_metertype.selectedIndex].ba_meter_id;
					}
					else
					{
						if ( global.setComboBoxNoSelection == true )
						{
							fltr_metertype.selectedIndex = -1;
						}
						fields["bam_type_id"] = "";
					}
					types["bam_type_id"] = "EXACT_STRING";
					
					if (fltr_qtytype.selectedIndex > -1) 
					{
						fields["bam_qty_type"] = qtyTypeList[fltr_qtytype.selectedIndex].qty_id;
					}
					else
					{
						if ( global.setComboBoxNoSelection == true )
						{
							fltr_qtytype.selectedIndex = -1;
						}
						fields["bam_qty_type"] = "";
					}
					types["bam_qty_type"] = "EXACT_STRING";
					
				}
				
				currentFilter = "WHERE 1=1 ";
				
				currentFields = fields;
				currentTypes = types;
				
				this.startLoadingPages();
				
				trace("this.grid.scroller3333", this.dataGrid.scroller);
				trace("this.grid.scroller.verticalScrollBar.value", this.dataGrid.scroller.verticalScrollBar.value);
			}
			
