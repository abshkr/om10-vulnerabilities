				this._saveButtonClicked = false;
			}
			
			private function checkFields():int
			{
				var errors:String="";
				var index:int=0;
				
				
				if ( StringUtil.trim(bam_kfa.text).length> 0  )
				{
					var kfa:Number;
					
					kfa = Number(StringUtil.trim(bam_kfa.text));
					if ( kfa < this.minKFA || kfa > this.maxKFA )
					{
						index += 1;
						errors += String(index) + ". " + resourceManager.getString('default','LOADMETER.PROMOT.BAM_KFA') + "[" + String(this.minKFA) + " ~ " + String(this.maxKFA) + "]\n";
					}
					
				}
				
				if ( StringUtil.trim(bam_min_flow.text).length> 0  )
				{
					var minflow:Number;
					
					minflow = Number(StringUtil.trim(bam_min_flow.text));
					if ( minflow < this.minMinFlow || minflow > this.maxMinFlow )
					{
						index += 1;
						errors += String(index) + ". " + resourceManager.getString('default','LOADMETER.PROMOT.BAM_MIN_FLOW') + "[" + String(this.minMinFlow) + " ~ " + String(this.maxMinFlow) + "]\n";
					}
					
				}
				
				if ( StringUtil.trim(bam_max_flow.text).length> 0  )
				{
					var maxflow:Number;
					
					maxflow = Number(StringUtil.trim(bam_max_flow.text));
					if ( maxflow < this.minMaxFlow || maxflow > this.maxMaxFlow )
					{
						index += 1;
						errors += String(index) + ". " + resourceManager.getString('default','LOADMETER.PROMOT.BAM_MAX_FLOW') + "[" + String(this.minMaxFlow) + " ~ " + String(this.maxMaxFlow) + "]\n";
					}
					
				}
				
				if ( StringUtil.trim(bam_min_flow.text).length> 0 && StringUtil.trim(bam_max_flow.text).length> 0  )
				{
					minflow = Number(StringUtil.trim(bam_min_flow.text));
					maxflow = Number(StringUtil.trim(bam_max_flow.text));
					if ( maxflow < minflow )
					{
						index += 1;
						errors += String(index) + ". " + resourceManager.getString('default','LOADMETER.PROMOT.BAM_SEQ_FLOW') + "\n";
					}
					
				}
				
				if ( index > 0 )
				{
					var errorBox:MessageDialog = new MessageDialog( errors, 400 );
				}
				
				return index;
			}

			private function createRecord():void
			{
				
				if ( this.checkFields() > 0 )
				{
					this.btn_save.enabled = this.canCreate;
					return;
				}
				
				var obj:Object = new Object();
				
				
				obj.bam_code 			= bam_code.text;
				obj.bam_name 			= bam_name.text;
				obj.bam_min_flow 		= bam_min_flow.text;
				obj.bam_max_flow 		= bam_max_flow.text;
				obj.bam_kfa 			= bam_kfa.text;
				
				if (bam_usage_id.selectedIndex>=0)
				{
					obj.bam_usage_id = loadMeterUsageList[bam_usage_id.selectedIndex].bam_usage_id;
				}
				else
				{
					obj.bam_usage_id = loadMeterUsageList[0].bam_usage_id;
				}
				
				
				if (bam_type_id.selectedIndex>=0)
				{
					obj.bam_type_id = meterTypeList[bam_type_id.selectedIndex].ba_meter_id;
				}
				else
				{
					obj.bam_type_id = "";//meterTypeList[0].ba_meter_id;
				}
				
				if (bam_qty_type.selectedIndex>=0)
				{
					obj.bam_qty_type = qtyTypeList[bam_qty_type.selectedIndex].qty_id;
				}
				else
				{
					obj.bam_qty_type = qtyTypeList[0].qty_id;
				}
				
				createAction.service( obj );
			}
			
			protected function createData_resultHandler( ):void
			{
				clearDetail();
				if ( this.createAction.dataString == "OK" )
				{
					global.msgSuccess(resourceManager.getString('default','MSG_CREATE__SUCCESS'));
				}
				else
				{
					global.msgFail(resourceManager.getString('default','ERROR_CREATE__FAILURE'));
				}
				
				var code:String;
				code = bam_code.text;
				
				currentState="normal";
				findByPrimaryKey( code );
				
				dataGrid.selectedIndex = -1;
				dataGrid.enabled=true;
			}
			
